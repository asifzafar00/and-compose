<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:color="#ffffff"
            android:padding="1dp"
            android:shape="oval">
            <corners
                android:bottomLeftRadius="@dimen/_40dp"
                android:bottomRightRadius="@dimen/_40dp"
                android:topLeftRadius="@dimen/_40dp"
                android:topRightRadius="@dimen/_40dp" />
        </shape>
        <stroke
            android:width="1dp"
            android:color="#e9e9e9" />

        <padding
            android:bottom="5dp"
            android:left="5dp"
            android:right="5dp"
            android:top="5dp" />
    </item>
    <item android:drawable="@drawable/creenshot" />
</layer-list>